{
    "name": "weeshr",
    "version": "1.0.0",
    "private": true,
    "description": "weeshr",
    "main": "src/index",
    "scripts": {
        "prestart": "npm run build",
        "start": "node -r ./scripts/tsconfig-path-bootstrap.js dist",
        "dev": "npx nodemon -e ts --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
        "lint": "eslint --ext .ts,.json --max-warnings=0 src",
        "test": "cross-env NODE_ENV=test PORT=8080 nyc --reporter=lcov --reporter=text --reporter=text-summary mocha -r ts-node/register -r tsconfig-paths/register src/test/index.test.ts --exit",
        "build": "rm -rf ./dist && npm i && npx tsc && cp -r ./src/rpc/pb ./dist/rpc/ && cp -r ./src/utils/templates ./dist/utils/templates && rm -rf ./node_modules/ && npm i --only=prod",
        "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
        "cool": "node --inspect -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
        "cool:watch": "nodemon"
    },
    "keywords": [
        "typescript",
        "jsdoc",
        "express",
        "eslint",
        "bootstrap"
    ],
    "author": "jeanvaljean",
    "license": "ISC",
    "dependencies": {
        "@types/chai-http": "^4.2.0",
        "@types/fs-extra": "^9.0.1",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mongoose-paginate-v2": "^1.6.5",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.19.0",
        "chai-http": "^4.3.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^8.6.0",
        "express": "^4.17.1",
        "express-validator": "^6.5.0",
        "fs-extra": "^9.0.1",
        "helmet": "^3.22.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "moment": "^2.26.0",
        "mongoose": "^5.9.15",
        "mongoose-paginate-v2": "^1.7.23",
        "morgan": "^1.10.0",
        "pm2": "^5.2.0",
        "tsconfig-paths": "^3.9.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/bluebird": "^3.5.31",
        "@types/chai": "^4.2.11",
        "@types/chai-as-promised": "^7.1.2",
        "@types/compression": "^1.7.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.6",
        "@types/express-validator": "^3.0.0",
        "@types/helmet": "0.0.47",
        "@types/lodash": "^4.14.153",
        "@types/mocha": "^7.0.2",
        "@types/mongoose": "^5.7.20",
        "@types/morgan": "^1.9.0",
        "@types/node": "^14.18.54",
        "better-docs": "^2.0.1",
        "chai": "^4.3.6",
        "chai-as-promised": "^7.1.1",
        "copy-files": "^0.1.0",
        "eslint": "^7.0.0",
        "eslint-config-google": "^0.14.0",
        "faker": "^4.1.0",
        "gh-pages": "^2.2.0",
        "jsdoc": "^3.6.4",
        "mocha": "^7.2.0",
        "nodemon": "^2.0.4",
        "nyc": "^15.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
}
