        "nodemailer-smtp-transport": "^2.7.4",
        "pg-promise": "^11.4.3",
        "pm2": "^5.2.0",
        "request": "^2.88.2",
        "request-promise": "^4.2.5",
        "ts-node": "^8.10.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.9.2",
