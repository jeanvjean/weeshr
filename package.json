{
    "name": "boiler",
    "version": "1.0.0",
    "private": true,
    "description": "Boiler",
    "main": "src/index",
    "typings": "src/index",
    "scripts": {
        "prestart": "npm run build",
        "start": "node -r ./scripts/tsconfig-path-bootstrap.js dist",
        "dev": "npx nodemon -e ts --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
        "create:migration": "node node_modules/db-migrate/bin/db-migrate --config database.json create",
        "migrate_down": "db-migrate reset -c 100 --config database.json",
        "migrate_up": "db-migrate up --config database.json",
        "create:seed": "db-migrate create:seed -c 100 --config database.json",
        "seed:down": "db-migrate reset:seed -c 100 --config database.json",
        "seed:up": "db-migrate up:seed --config database.json",
        "migrate:fresh": "npm run migrate_down && npm run migrate_up && npm run seed:down && npm run seed:up",
        "lint": "eslint --ext .ts,.json --max-warnings=0 src",
        "test": "NODE_ENV=test nyc mocha -r ts-node/register -r tsconfig-paths/register src/test/**/*.ts --exit",
        "test:unit": "NODE_ENV=test nyc mocha -r ts-node/register -r tsconfig-paths/register --grep @unit src/**/*.spec.ts",
        "build": "rm -rf ./dist && npm i && npx tsc && cp -r ./src/rpc/pb ./dist/rpc/ && cp -r ./src/utils/templates ./dist/utils/templates && rm -rf ./node_modules/ && npm i --only=prod",
        "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
        "deploy": "gh-pages -d docs",
        "cool": "node --inspect -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
        "cool:watch": "nodemon"
    },
    "keywords": [
        "typescript",
        "jsdoc",
        "express",
        "eslint",
        "bootstrap"
    ],
    "author": "jeanvaljean",
    "license": "ISC",
    "dependencies": {
        "@grpc/proto-loader": "^0.5.4",
        "@types/chai-http": "^4.2.0",
        "@types/db-migrate-pg": "^0.0.10",
        "@types/fs-extra": "^9.0.1",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mongoose-paginate-v2": "^1.6.5",
        "@types/nodemailer": "^6.4.4",
        "@types/nodemailer-smtp-transport": "^2.7.5",
        "@types/pg-promise": "^5.4.3",
        "@types/request-promise": "^4.1.46",
        "@types/winston": "^2.4.4",
        "bcrypt": "^4.0.1",
        "bluebird": "^3.7.2",
        "body-parser": "^1.19.0",
        "chai-http": "^4.3.0",
        "cloudinary": "^1.22.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^8.6.0",
        "express": "^4.17.1",
        "express-fileupload": "^1.1.7-alpha.4",
        "express-validator": "^6.5.0",
        "fs-extra": "^9.0.1",
        "google-protobuf": "^3.12.2",
        "googleapis": "^52.1.0",
